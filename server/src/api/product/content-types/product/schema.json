{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "products"
    },
    "originalPrice": {
      "required": true,
      "type": "decimal"
    },
    "discountedPrice": {
      "type": "decimal"
    },
    "tag": {
      "type": "enumeration",
      "enum": ["akcija", "novo"]
    },
    "favoritedBy": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "favoriteProducts"
    },
    "productTypeId": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "productVariantId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "webAccountArticleName": {
      "type": "string",
      "required": true
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "model": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::model.model",
      "inversedBy": "products"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "subCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "material": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::material.material",
      "inversedBy": "products"
    },
    "memory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::memory.memory",
      "inversedBy": "products"
    },
    "stores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store.store",
      "inversedBy": "products"
    },
    "ancModel": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "keyboard": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "wifiModel": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "accessoriesType": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "braceletSize": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "productLink": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^(?!\\/)[a-zA-Z0-9\\-\\/_]+$"
    },
    "details": {
      "type": "blocks"
    },
    "chip": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::chip.chip",
      "inversedBy": "products"
    },
    "screenSize": {
      "type": "string"
    },
    "release_date": {
      "type": "string"
    },
    "numberOfCores": {
      "type": "integer"
    },
    "ram": {
      "displayName": "Ram",
      "type": "component",
      "repeatable": false,
      "component": "global.ram"
    }
  }
}
