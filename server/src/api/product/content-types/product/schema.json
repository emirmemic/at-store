{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "productId": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "products"
    }
  }
}
