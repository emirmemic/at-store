{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "displayName": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category"
    },
    "link": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^(?!\\/)[a-zA-Z0-9\\-\\/_]+$"
    },
    "startingPrice": {
      "type": "decimal",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "models": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::model.model",
      "mappedBy": "category"
    },
    "chips": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chip.chip",
      "mappedBy": "category"
    },
    "subCategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-category.sub-category",
      "mappedBy": "category"
    },
    "metaTitle": {
      "type": "string",
      "maxLength": 255
    },
    "metaDescription": {
      "type": "string",
      "maxLength": 255
    }
  }
}
