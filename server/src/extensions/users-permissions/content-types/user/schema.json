{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "confirmedByAdmin": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false,
      "maxLength": 30,
      "minLength": 3
    },
    "surname": {
      "type": "string",
      "required": true,
      "maxLength": 30,
      "minLength": 3
    },
    "address": {
      "type": "string",
      "maxLength": 50,
      "required": false
    },
    "companyName": {
      "type": "string"
    },
    "companyIdNumber": {
      "type": "biginteger",
      "required": false,
      "unique": true,
      "min": "13",
      "max": "13"
    },
    "phoneNumber": {
      "type": "biginteger",
      "required": true,
      "max": "12",
      "min": "8"
    },
    "favoriteProducts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "favoritedBy"
    },
    "payment_methods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment-method.payment-method",
      "mappedBy": "user"
    }
  }
}
